{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "development-apk": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-apk"
    }
  },
  "submit": {
    "production": {}
  }
}
